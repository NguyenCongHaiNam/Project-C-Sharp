@{
    ViewData["Title"] = "About Our Project";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>

<div class="container mt-5">
    <h1 class="text-center">@ViewData["Title"]</h1>
    <div class="row mt-4">
        <div class="col-md-6 offset-md-3">
            <div class="card shadow-lg">
                <div class="card-body">
                    <h2 class="card-title">Our Project Team</h2>
                    <ul class="list-group list-group-flush">
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Nguyen Cong Hai Nam
                                <span class="badge badge-primary badge-pill">Team Leader</span>
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Vu Cong Thieu
                                <span class="badge badge-secondary badge-pill">Team Member</span>
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                To Khanh Dung
                                <span class="badge badge-secondary badge-pill">Team Member</span>
                            </li>
                        </ul>

                    </ul>
                    <p class="card-text mt-3">We are the team behind the development and management of this C# project. Join us to create a wonderful online experience!</p>
                </div>
            </div>
            <div class="card shadow-lg mt-4">
                <div class="card-body">
                    <h2 class="card-title">Instructor</h2>
                    <p class="card-text">Our project is supervised by [Instructor's Name].</p>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

</body>
</html>
