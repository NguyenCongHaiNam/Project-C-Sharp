@* @model List<User> *@
@{
    ViewBag.Title = "index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<h2>User Manager</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <!-- Thêm các cột khác nếu cần -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.IdUser</td>
                <td>@user.Email</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <!-- Thêm các cột khác nếu cần -->
            </tr>
        }
    </tbody>
</table>
